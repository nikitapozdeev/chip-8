{
  "compilerOptions": {
    "outDir": "./build/",
    "noImplicitAny": true,
    "module": "NodeNext",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": false,
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
  },
  "include": [
    "src",
  ]
}